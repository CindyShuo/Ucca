/* index/code.wxss */
@import "my1.wxss";
@import "window1.wxss";

.code-bg {
  width:600rpx;
  height:950rpx;
  margin:40rpx 75rpx;
  position: relative;

}
.share{
  width:34rpx;
  height:34rpx;
  position: absolute;
  top:90rpx;
  right:50rpx;
}
.code-font{
  position: absolute;
  top:50rpx;
  left:50rpx;
}
.ma{
  position:absolute;
  width:340rpx;
  height:340rpx;
  left:130rpx;
  top:230rpx;
}
.code-line{
  width:540rpx;
  height:2rpx;
  background: rgba(0,0,0,.1);
  position: absolute;
  left:30rpx;
  bottom:135rpx;
}
.code-font1{
  position:absolute;
  left:40rpx;
  right:40rpx;
  bottom:30rpx;
  height:76rpx;
  width:520rpx;
}
.code-font1-gray{
  font-size:24rpx;
  opacity: .6;
  line-height:33rpx;
  text-align: left;
}
.code-arrow{
  position:absolute;
  top:20rpx;
  right:0;
  margin:0;
}
.code-font2{
  position:absolute;
  font-size:28rpx;
  top:14rpx;
  right:38rpx;
}
.code-ps{
  opacity: .6;
  font-size:24rpx;
  line-height: 33rpx;
  width:560rpx;
  margin-left:95rpx;
}
.code-ps-topic{
  opacity: 1;
  font-weight: bold;
  line-height: 40rpx;
}
.code-bottom{
  position: fixed;
  width:100%;
  height:100rpx;
  background: #fff;
  font-size:30rpx;
  text-align:center;
  line-height:100rpx;
  bottom:0;
}
.code-bottom-div{
  float:left;
  width:375rpx;
  border-right:1rpx solid rgba(0,0,0,.1);
}
.code-bottom-div1{
  border:none;
}
.my-list-image{
  position: relative;
  float:left;
  width:600rpx;
  height:200rpx;
  top:0rpx;
  left:0rpx;
}
.my-list-mask{
  width:600rpx;
  height:200rpx;
}
.image-left-bottom {
  left: -1rpx;
  bottom: -1rpx;
  border-radius: 0 20rpx 0 0;
}

.image-right-bottom {
  right: -1rpx;
  bottom: -1rpx;
  border-radius: 20rpx 0 0 0;
}
.code-con{
  float:left;
  position: relative;
  width:600rpx;
  height:750rpx;
  background: #fff;
  box-shadow: 0rpx 0rpx 5rpx rgba(0,0,0,.1);
}
.logo{
  width:80rpx;
  height:80rpx;
  position: absolute;
  top:60rpx;
  left:50rpx;
}