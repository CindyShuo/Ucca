/* index/win4.wxss */
@import "window1.wxss";
.win4{
  width:560rpx;
  height:550rpx;
  background: #fff;
  border-radius: 10rpx;
  position: absolute;
  left:50%;
  top:50%;
  margin-left:-280rpx;
  margin-top:-275rpx;
}
.ok-icon{
  width:200rpx;
  height:200rpx;
