/* index/me-infor.wxss */
@import "index.wxss";
.infor-list{
width:650rpx;
padding:50rpx;
font-size:28rpx;
line-height: 40rpx;
background: #fff;
font-weight: bold;
margin-bottom: 30rpx;
}
.infor-list:first-child{
  border-top: 1rpx solid rgba(0,0,0,.1)
}
.infor-list-div{
  width:100%;
  height:40rpx;
  margin-bottom:60rpx;
}
.infor-list-div1{
  height:80rpx;
  line-height: 80rpx;
}
.infor-list-div1 .infor-list-right{
  width:80rpx;
  height:80rpx;
}
.infor-list-div1 .infor-list-left{
  height:80rpx;
}
.infor-list-div:last-child{
  margin-bottom:0;
}
.infor-list-left{
  width:50%;
  float:left;
}
.infor-list-right{
  float:right;
}